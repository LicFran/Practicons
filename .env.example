# Pr√°ctiCos Environment Variables
# Copy this file to .env and modify as needed

# OCR Configuration
TESSERACT_PATH=tesseract  # Path to Tesseract executable
OCR_LANGUAGE=spa  # Language for OCR (spa for Spanish, eng for English, etc.)
OCR_DPI=300  # DPI for image processing

# AI Configuration
OPENAI_API_KEY=  # Your OpenAI API key
USE_AI_EXTRACTION=True  # Set to False to disable AI extraction

# Paths can be configured here if needed
# CUSTOM_INPUT_DIR=path/to/input
# CUSTOM_OUTPUT_DIR=path/to/output 